{
  "name": "kredo",
  "description": "- To run in development run the commands as follows:",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cd ./emailing && node emailTests.js",
    "api": "cd backend && npm start",
    "client": "cd kredo && yarn dev",
    "api:dev": "nodemon server.js",
    "start": "node server.js",
    "restart": "pm2 restart server.js",
    "preinstall": "npm run gen:keypair",
    "server-build": "cd kredo && build:rollup",
    "build": "cd kredo && yarn build",
    "gen:keypair": "node ./cryptography/generateKeypair.js",
    "postinstall": "cd kredo && yarn build",
    "run:migrations": "db-migrate create initialize --sql-file",
    "migrate": "sequelize db:migrate",
    "test:email": "node ./emailing/emailTests.js"
  },
